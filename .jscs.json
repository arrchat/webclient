{
  "preset": "airbnb",
  "disallowVar": true,
  "disallowMultipleVarDecl": false,
  "jsDoc": {
    "checkAnnotations": {
      "extra": {
        "class": true,
        "param": true,
        "example": true,
        "module": true,
        "func": true,
        "alias": true,
        "namespace": true,
        "memberof": true,
        "instance": true,
        "type": true,
        "returns": true
      }
    }
  }
}
